{
  "name": "react-metaforms",
  "version": "2.0.10",
  "main": "lib/export",
  "typings": "lib/export",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flsy/react-metaforms.git"
  },
  "keywords": [
    "react",
    "forms"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flsy/react-metaforms/issues"
  },
  "homepage": "https://flsy.github.io/react-metaforms",
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"src/**/*.{js,jsx}\"",
    "prettier": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "tslint -c ./tslint.json './src/**/*.{ts,tsx}'",
    "build": "rm -rf ./lib && tsc --p tsconfig.json",
    "verify": "npm run format && npm run lint && CI=true npm test && npm run build",
    "prepublishOnly": "npm run build",
    "predeploy": "build-storybook -s public",
    "deploy": "NODE_DEBUG=gh-pages gh-pages -d storybook-static",
    "storybook": "start-storybook -p 9009 -s public"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "fputils": "1.0.12",
    "metaforms": "0.0.10",
    "react": "16.*"
  },
  "devDependencies": {
    "@storybook/addon-actions": "5.3.18",
    "@storybook/addon-links": "5.3.18",
    "@storybook/addons": "5.3.18",
    "@storybook/preset-create-react-app": "2.1.1",
    "@storybook/react": "5.3.18",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "25.1.5",
    "@types/node": "13.11.0",
    "@types/react": "16.9.32",
    "@types/react-dom": "16.9.6",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "gh-pages": "2.2.0",
    "prettier": "2.0.2",
    "react-dom": "16.13.1",
    "react-scripts": "3.4.1",
    "tslint": "6.1.1",
    "tslint-react": "4.2.0",
    "typescript": "3.8.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
