{
  "name": "react-metaforms",
  "version": "2.2.3",
  "main": "lib/export",
  "typings": "lib/export",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flsy/react-metaforms.git"
  },
  "keywords": [
    "react",
    "forms"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flsy/react-metaforms/issues"
  },
  "homepage": "https://flsy.github.io/react-metaforms",
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "start-storybook -p 9009",
    "test": " jest",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"src/**/*.{js,jsx}\"",
    "prettier": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "tslint -c ./tslint.json './src/**/*.{ts,tsx}'",
    "build": "rm -rf ./lib && tsc --p tsconfig.json",
    "verify": "npm run format && npm run lint && CI=true npm test && npm run build",
    "prepublishOnly": "npm run build",
    "predeploy": "build-storybook",
    "deploy": "NODE_DEBUG=gh-pages gh-pages -d storybook-static"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "fputils": "1.0.12",
    "metaforms": "0.2.2",
    "react": "16.*"
  },
  "devDependencies": {
    "@babel/core": "7.10.3",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-links": "5.3.19",
    "@storybook/addons": "6.0.5",
    "@storybook/preset-create-react-app": "3.1.3",
    "@storybook/react": "5.3.19",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "26.0.9",
    "@types/node": "14.0.27",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "babel-loader": "8.1.0",
    "babel-preset-react-app": "9.1.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "gh-pages": "3.1.0",
    "jest": "25.5.4",
    "prettier": "2.0.5",
    "react-dom": "16.13.1",
    "ts-jest": "25.5.1",
    "tslint": "6.1.3",
    "tslint-react": "5.0.0",
    "typescript": "3.9.6"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/lib"
    ],
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  }
}
